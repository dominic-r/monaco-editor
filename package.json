{
  "name": "@hey-web-components/monaco-editor",
  "version": "0.1.0-pre7",
  "type": "module",
  "description": "A web component wrapper for monaco-editor.",
  "repository": {
    "type": "git",
    "url": "https://github.com/hey-web-components/monaco-editor.git"
  },
  "main": "dist/node/index.js",
  "exports": {
    ".": "./dist/node/index.js"
  },
  "types": "types/index.d.ts",
  "unpkg": "dist/cdn/index.js",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && vite build -c vite.node.config.ts",
    "build:cdn": "tsc && vite build",
    "build:node": "tsc && vite build -c vite.node.config.ts",
    "analyze": "cem analyze",
    "storybook": "npm run analyze && NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006",
    "storybook:build": "npm run analyze && NODE_OPTIONS=--openssl-legacy-provider  build-storybook",
    "prepare": "npm run build"
  },
  "dependencies": {
    "lit": "^2.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@custom-elements-manifest/analyzer": "^0.6.8",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-vite": "^0.3.0",
    "@storybook/web-components": "^6.5.16",
    "babel-loader": "^8.3.0",
    "lit-html": "^2.6.1",
    "monaco-editor": "^0.34.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  },
  "license": "MIT",
  "keywords": [
    "monaco",
    "editor",
    "monaco-editor",
    "angular",
    "react",
    "vue",
    "js",
    "web",
    "component",
    "webcomponent"
  ]
}
